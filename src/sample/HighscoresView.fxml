<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HighscoresViewController">
    <Label layoutX="184.0" layoutY="33.0" text="Highscores">
        <font>
          <Font size="43.0" />
        </font>
    </Label>
    <TableView fx:id="tableView" layoutX="60.0" layoutY="106.0" prefHeight="189.0" prefWidth="478.0">
      <columns>
          <TableColumn fx:id="rankColumn" editable="false" minWidth="50.0" prefWidth="75.0" sortable="false" text="Rank" />
          <TableColumn fx:id="nameColumn" editable="false" minWidth="200.0" prefWidth="75.0" sortable="false" text="Name" />
          <TableColumn fx:id="scoreColumn" editable="false" minWidth="200.0" prefWidth="75.0" sortable="false" text="Moves" />
      </columns>
    </TableView>
   <Button layoutX="273.0" layoutY="336.0" mnemonicParsing="false" onAction="#menuButton_pressed" text="Menu" />
</AnchorPane>
